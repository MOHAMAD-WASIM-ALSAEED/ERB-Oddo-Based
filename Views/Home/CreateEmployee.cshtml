@section Scripts{
    <script>
        $(function () {
            $(".chzn-select").chosen();
            $('.datepicker').datepicker();
        });
        var count = 0;
        var dependentCount = 0;
        var arr = new Array();
        function Delete(id) {
            $('#' + id).remove();
            arr = arr.filter(item => item !== id);
            dependentCount--;
            $('#countdependant').text(dependentCount)
        }
        function create() {
            count++;
            $('#mcontent').append(
                '<tr id="' + count + '">' +
                '<td><input id="name_' + count + '" type="text" placeholder="name"/></td>' +
                '<td><input id="birth_' + count + '" type="date" /></td>' +
                '<td><a href="javascript:;" onclick="Delete(' + count + ')" ><i class="fa fa-trash"></i></a></td>' +
                '</tr>');
            arr.push(count);
            dependentCount++;
            $('#countdependant').text(dependentCount)
        }


        function getDate() {
            var obj = new Array();
            for (let index = 0; index < arr.length; index++) {
                obj.push({
                    Name: $('#name_' + arr[index]).val(),
                    BirthDate: $('#birth_' + arr[index]).val(),
                });

            };

            console.log(obj);
            $.post('/home/SaveChildren', JSON.stringify(obj), 'json')

          
        }

    </script>
}










