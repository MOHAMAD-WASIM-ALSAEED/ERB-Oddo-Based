@section Scripts{

    @if (Model.EmployeeDependents.Count > 0)
    {
        <script>
            var count = parseInt('@Model.EmployeeDependents.Count');
            var arr = new Array();
            var cnt = 1;
            for (var i = 0; i < count; i++) {
                arr.push(cnt);
                cnt++;
            }
        </script>

    }
    else
    {
        <script>
            var count = 0;
            var arr = new Array();
        </script>
    }
    <script>

        $(function () {
            $(".chzn-select").chosen();
            $('.datepicker').datepicker();
            function readURL(input) {
                if (input.files && input.files[0]) {
                    var reader = new FileReader();

                    reader.onload = function (e) {
                        $('#blah').attr('src', e.target.result);
                        $('#encode').attr('value', e.target.result);
                    }

                    reader.readAsDataURL(input.files[0]); // convert to base64 string
                }
            }
           
            $("#imgw").change(function () {
                readURL(this);
            });
        });
        function selectchange() {
            var selected = $('#mar option:selected').text();
            if (selected == "married") {
                $("#hide").attr("hidden", false)
            }
            else { $("#hide").attr("hidden", true) }
        }
        function DeleteImage() {

            $('#encode').val("");
            $('#imgw').val("");
            $('#blah').attr("src", "");

        }

        $('#choose-file1').inputFileText({
            text: 'Upload Your File'
        });
        $('#choose-file2').inputFileText({
            text: 'Upload Your File'
        });
        $('#choose-file3').inputFileText({
            text: 'Upload Your File'
        });
        $('#choose-file4').inputFileText({
            text: 'Upload Your File'
        });
        $('input[value="Upload Your File"]').addClass('btn btn-success p-0');
        var dependentCount = 0;

        function Delete(id) {
            $('#' + id).remove();
            arr = arr.filter(item => item !== id);
            dependentCount--;
            $('#countdependant').text(dependentCount)
        }
        function create() {
            count++;
            $('#mcontent').append(
                '<tr id="' + count + '">' +
                '<td><input id="name_' + count + '" type="text" placeholder="name"/></td>' +
                '<td><input id="birth_' + count + '" type="date" /></td>' +
                '<td><a href="javascript:;" onclick="Delete(' + count + ')" ><i class="fa fa-trash"></i></a></td>' +
                '<td><input hidden  type="date" /></td>' +
                '</tr>');
            arr.push(count);
            dependentCount++;
            $('#countdependant').text(dependentCount)
        }
        var obj = new Array();


        function SubmitForm() {


            obj = [];
            for (let index = 0; index < arr.length; index++) {
                obj.push({
                    Name: $('#name_' + arr[index]).val(),
                    Bdate: $('#birth_' + arr[index]).val(),
                    Id: parseInt($('#id_' + arr[index]).val())
                });
            }
            console.log(obj);
            $('#dependant').val(JSON.stringify(obj));
            $('#DataForm').submit();

